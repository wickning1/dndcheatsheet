{
  "name": "dndcheatsheet",
  "description": "A quick little display-only app that converts JSON character info into a printable sheet",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wickning1/dndcheatsheet.git"
  },
  "license": "MIT",
  "devDependencies": {
    "js-yaml": "^3.13.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.12.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^4.0.4",
    "svelte": "^3.0.0"
  },
  "dependencies": {
    "sirv-cli": "^0.4.4"
  },
  "scripts": {
    "build": "rollup -c",
    "autobuild": "rollup -c -w",
    "dev": "run-p start:dev autobuild",
    "start": "sirv public --single",
    "start:dev": "sirv public --single --dev"
  }
}
